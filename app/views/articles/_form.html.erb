<%= render 'errors' %>
<%# articles_path is a short hand for /articles %>
<%= form_with(model: @article, data: {turbo: false}, local: true) do |f|%>
  <p><%= f.label :title %></p>
  <p><%= f.text_field :title %></p>

  <p><%= f.label :description %></p>
  <p><%= f.text_area :description %></p>

  <p><%= f.submit %></p>
<% end %>